	year = {2010},
	doi = {10.1007/978-3-642-15340-2_12},
	keywords = {consumption, France, wealth effect},
	pages = {263--282},
	file = {Springer Full Text PDF:/home/gpetrini/Zotero/storage/GMG6YDX6/Chauvin e Damette - 2010 - Wealth Effects on Private Consumption the French .pdf:application/pdf}
}

