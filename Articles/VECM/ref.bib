	file = {Schettini et al. - 2012 - Novas evidências empíricas sobre a dinâmica trimes.pdf:/home/gpetrini/Zotero/storage/4L22UWUZ/Schettini et al. - 2012 - Novas evidências empíricas sobre a dinâmica trimes.pdf:application/pdf}
