  <script>
  $(document).ready(function() {
      $('.d2h-file-wrapper').each(function(i, wrapper) {
          var $wrapper = $(wrapper);
          var lang = $wrapper.data('lang');
          $wrapper.find('.d2h-code-line-ctn').each(function(i, line) {
              var $line = $(line);
              $line.addClass(lang);
              //hljs.highlightBlock(line);
      });
    });
  });
