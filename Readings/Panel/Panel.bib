	doi = {10.1111/j.1368-423X.2007.00227.x},
	keywords = {A Bias-Adjusted LM Test of Error Cross-Section Independence, Aman Ullah, Cross-sectional Dependence test, Hypothesis test, M. Hashem Pesaran, SSRN, Takashi Yamagata},
	file = {Pesaran et al. - 2007 - A Bias-Adjusted LM Test of Error Cross-Section Ind.pdf:/home/gpetrini/Zotero/storage/5S3W64VT/Pesaran et al. - 2007 - A Bias-Adjusted LM Test of Error Cross-Section Ind.pdf:application/pdf;Snapshot:/home/gpetrini/Zotero/storage/2J8YLUG2/papers.html:text/html}
