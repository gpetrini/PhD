@article{pesaran_testing_2008,
	title = {Testing slope homogeneity in large panels},
	volume = {142},
	issn = {0304-4076},
	url = {http://www.sciencedirect.com/science/article/pii/S0304407607001224},
	doi = {10.1016/j.jeconom.2007.05.010},
	abstract = {This paper proposes a standardized version of Swamy's test of slope homogeneity for panel data models where the cross section dimension (N) could be large relative to the time series dimension (T). The proposed test, denoted by Δ˜, exploits the cross section dispersion of individual slopes weighted by their relative precision. In the case of models with strictly exogenous regressors, but with non-normally distributed errors, the test is shown to have a standard normal distribution as (N,T) →j∞ such that N/T2→0. When the errors are normally distributed, a mean-variance bias adjusted version of the test is shown to be normally distributed irrespective of the relative expansion rates of N and T. The test is also applied to stationary dynamic models, and shown to be valid asymptotically so long as N/T→κ, as (N,T)→j∞, where 0⩽κ{\textless}∞. Using Monte Carlo experiments, it is shown that the test has the correct size and satisfactory power in panels with strictly exogenous regressors for various combinations of N and T. Similar results are also obtained for dynamic panels, but only if the autoregressive coefficient is not too close to unity and so long as T⩾N.},
	language = {en},
	number = {1},
	urldate = {2020-04-22},
	journal = {Journal of Econometrics},
	author = {Pesaran, M. Hashem and Yamagata, Takashi},
	month = jan,
	year = {2008},
	keywords = {Dispersion tests, Heterogenity test, Hypothesis test, Large panels, Monte Carlo results, Tests of slope homogeneity},
	pages = {50--93},
	file = {ScienceDirect Snapshot:/home/gpetrini/Zotero/storage/WBPCI4HK/S0304407607001224.html:text/html;Versão submetida:/home/gpetrini/Zotero/storage/VRN72CU9/Hashem Pesaran e Yamagata - 2008 - Testing slope homogeneity in large panels.pdf:application/pdf}
